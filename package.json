{
  "name": "echarts-extension-leaflet",
  "version": "1.0.0",
  "description": "An Leaflet(https://leafletjs.com/) extension for Apache ECharts (https://github.com/apache/echarts)",
  "main": "dist/echarts-extension-leaflet.min.js",
  "module": "dist/echarts-extension-leaflet.esm.js",
  "files": [
    "dist",
    "src",
    "index.js",
    "index.d.ts",
    "export.js",
    "export.d.ts",
    "types.d.ts",
    "examples/*.html"
  ],
  "types": "types.d.ts",
  "scripts": {
    "dev": "rollup -wc --environment NODE_ENV:development",
    "build": "rollup -c --environment NODE_ENV:production",
    "release": "npm run build && npm run generate:example",
    "generate:example": "node build/generateExamples.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joakimono/echarts-extension-leaflet.git"
  },
  "keywords": [
    "echarts",
    "leaflet",
    "echarts-extension",
    "data-visualization",
    "map",
    "echarts-leaflet",
    "echarts5"
  ],
  "author": "joakimono",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joakimono/echarts-extension-leaflet/issues"
  },
  "homepage": "https://github.com/joakimono/echarts-extension-leaflet#readme",
  "peerDependencies": {
    "echarts": "^5.0.1",
    "leaflet": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/leaflet": "^1.7.6",
    "chalk": "^4.1.0",
    "echarts": "^5.0.1",
    "leaflet": "^1.7.1",
    "rollup": "^2.39.1",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "parcel": "^2.0.1"
  }
}
